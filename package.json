  "devDependencies": {
    "@types/bun": "latest",
    "prisma": "^6.6.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@bogeychan/elysia-oauth2": "^0.1.0",
